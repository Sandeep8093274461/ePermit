<%- include('common/header.ejs') -%>
<div class="content-wrapper">
    <script type="text/javascript" src="../../javascripts/jquery.min.js"></script>
    <section class="content">


        <form name="myForm" novalidate ng-show="showAppForm">

            <div class="panel panel-primary" id="allShow" style="color: blue; border-radius: 30px;">
                <div class="panel-heading" style="border-radius: 30px;">
                    <span><strong>
                            <center>APPLICATION FORM FOR INNER LINE PERMIT (ILP) EXEMPTION CARD<br>
                            </center>
                        </strong>
                        <center> Home Department, Notification NO.CON-3/PAP/65/10, Dated Kohima, the 9th December, 2019,
                            <br>
                            extending Inner Line to cover the entire District of Dimapur.
                        </center>
                    </span>
                </div><br>
                <div class="panel panel-primary" id="allShow" class="tab-pane fade in active"
                    style="border-radius: 30px;">
                    <div class="panel-heading" style="border-radius: 30px;">
                        <span><strong>
                                <center>User Details</center>
                            </strong>
                        </span>
                    </div>
                    <div class="panel-body">
                        <style>
                            input {
                                text-transform: uppercase
                            }

                            textarea {
                                text-transform: uppercase;
                            }
                        </style>

                        <div class="row">

                            <div class="col-md-3"><label for="name"><b>Enter Token Number<span
                                            style="color: red">*</span>:</b></label></div>
                            <div class="col-md-5">

                                <!-- <span ng-show="myForm.tokenNo.$error.pattern" style="color:red">Enter Your Valid Token Number.</span> -->
                                <input type="text" class="form-control" name="tokenNo" ng-model="tokenNo"
                                    placeholder="Enter Token Number" maxlength="6" minlength="6" required
                                    ng-change="validateToken()" required>
                                <span ng-show="tokenno" style="color:red">Token Number already used by {{userId}}
                                    .</span>
                                <span style="color:red" ng-show="myForm.tokenNo.$dirty && myForm.tokenNo.$invalid ">
                                    <span ng-show="myForm.tokenNo.$error.required">Token Number is Required
                                        .</span>
                                </span>
                            </div>
                        </div><br />

                        <div class="row">
                            <div class="table col-lg-12 col-xs-12 ">
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="name"><b>Name<span style="color: red">*</span>:</b></label><br>
                                        <div class="col-md-3" style="padding: 0%;">
                                            <select class="form-control" name="ntitle" ng-model="ntitle"
                                                ng-disabled="tokenno" required>

                                                <option value="Shri."> SHRI</option>
                                                <option value="Smti."> SMTI</option>
                                                <option value="Miss."> MISS</option>
                                                <option value="Kumari."> KUMARI</option>
                                                <option value="Master."> MASTER</option>
                                            </select>

                                            </span>
                                        </div>
                                        <div class="col-md-9" style="padding: 0%;">
                                            <input type="text" class="form-control" name="name" ng-model="name"
                                                placeholder="Enter Your name" onkeypress='nameFilter(event)'
                                                ng-disabled="tokenno" required>
                                            <span style="color:red"
                                                ng-show="myForm.name.$dirty && myForm.name.$invalid">
                                                <span ng-show="myForm.name.$error.required">Name is Required .</span>
                                            </span>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Father Name<span
                                                    style="color: red">*</span>:</b></label><br>
                                        <div class="col-md-3" style="padding: 0%;">
                                            <select class="form-control" name="fatherTitle" ng-model="fatherTitle"
                                                required>
                                                <option value="Shri." selected> SHRI</option>
                                                <option value="Late."> LATE</option>
                                            </select>

                                            </span>
                                        </div>
                                        <div class="col-md-9" style="padding: 0%;">
                                            <input type="text" class="form-control" name="fathername"
                                                ng-model="fathername" placeholder="Enter Fathers name" required>
                                            <span style="color:red"
                                                ng-show="myForm.fathername.$dirty && myForm.fathername.$invalid">
                                                <span ng-show="myForm.fathername.$error.required">Fathers Name is
                                                    required.</span>
                                            </span>
                                        </div>

                                    </div>
                                    <div class="col-md-4">
                                        <label for="Phone"><b>Date Of Birth<span
                                                    style="color: red">*</span>:</b></label>
                                        <input type="date" class="form-control" name="dob" ng-model="dob" required>
                                        <span style="color:red" ng-show="myForm.dob.$dirty && myForm.dob.$invalid">
                                            <span ng-show="myForm.dob.$error.required">Date Of Birth is required.</span>
                                        </span>
                                    </div>
                                </div><br />
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="gender"><b>Gender<span style="color: red">*</span>:</b></label>
                                        <select class="form-control" name="gender" ng-model="gender" required>
                                            <option value="" disabled selected>--select--</option>
                                            <option value="Male"> MALE</option>
                                            <option value="Female"> FEMALE</option>
                                            <option value="Transgender"> TRANSGENDER</option>
                                        </select>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="name"><b>Identification Mark<span
                                                    style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" name="mark" ng-model="mark"
                                            placeholder="Enter Identification Mark" required>
                                        <span style="color:red" ng-show="myForm.mark.$dirty && myForm.mark.$invalid">
                                            <span ng-show="myForm.mark.$error.required">Enter Your Identification
                                                Mark.</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Contact Number:</b></label>
                                        <input type="text" class="form-control" name="contactno" ng-model="contactno"
                                            placeholder="Enter Contact numer" maxlength="10" minlength="10"
                                            ng-pattern="/^[1-9][0-9]{9}$/" allow-numbers-only
                                            onkeypress='validate(event)'>
                                        <span ng-show="myForm.contactno.$error.pattern" style="color:red">Invalid
                                            Contact Number.</span>
                                        <span style="color:red"
                                            ng-show="myForm.contactno.$dirty && myForm.contactno.$invalid">
                                            <span ng-show="myForm.contactno.$error.required">Enter Your Valid Phone
                                                Number.</span>
                                        </span>
                                    </div>


                                </div><br />
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Aadhaar Number:</b></label>
                                        <input type="text" class="form-control" name="addahar" ng-model="addahar"
                                            placeholder="Enter Aadhaar numer" maxlength="12" minlength="12"
                                            onkeypress='validate(event)' ng-pattern="/^[1-9][0-9]{11}$/"
                                            allow-numbers-only>
                                        <span ng-show="myForm.addahar.$error.pattern" style="color:red">Invalid
                                            Aadhar.</span>
                                        <span style="color:red"
                                            ng-show="myForm.addahar.$dirty && myForm.addahar.$invalid">
                                            <span ng-show="myForm.addahar.$error.required">Enter Your Aadhaar
                                                Number</span>
                                        </span>
                                    </div>

                                    <div class="col-md-4">
                                        <label for="fathername"><b>Photo Upload<span
                                                    style="color: red">*</span>:</b></label>
                                        <input type="file" class="form-control custom-file-input" accept="image/*"
                                            style="margin-left: 0px;" ng-model="photo" id="userPhoto" name="userPhoto"
                                            onchange="angular.element(this).scope().userPhotoCheck()">
                                        <div ng-show="showUserPhotoError">
                                            <span style="color: red;">{{userPhotoFileGood}}</span>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="Phone"><b>Marital Status:</b></label>
                                        <select class="form-control" ng-model="material" name="material">
                                            <option value="Married"> MARRIED</option>
                                            <option value="Unmarried"> UNMARRIED</option>
                                        </select>
                                    </div>
                                </div><br />
                                <div class="row" ng-if="material == 'Married'">
                                    <div class="col-md-4">
                                        <label for=""><b>Husband/Spouse Name<span
                                                    style="color: red">*</span>:</b></label><br>
                                        <div class="col-md-3" style="padding: 0%;">
                                            <select class="form-control" name="husband_spousetitle"
                                                ng-model="$parent.husband_spousetitle" required>
                                                <option value="" disabled selected>Select</option>
                                                <option value="Shri."> SHRI</option>
                                                <option value="Smti."> SMTI</option>
                                                <option value="Late."> LATE</option>
                                            </select>
                                        </div>
                                        <div class="col-md-9" style="padding: 0%;">
                                            <input type="text" class="form-control" name="husband_spouse"
                                                ng-model="$parent.husband_spouse"
                                                placeholder="Enter Husband Or Spouse name"
                                                onkeypress='nameFilter(event)' required>
                                            </span>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Father Name <span
                                                    style="color: red">*</span>:</b></label><br>
                                        <div class="col-md-3" style="padding: 0%;">
                                            <select class="form-control" ng-model="$parent.husband_spousefathertitle"
                                                name="husband_spousefathertitle" required>
                                                <option value="" disabled selected>select</option>
                                                <option value="Shri."> SHRI</option>
                                                <option value="Late."> LATE</option>
                                            </select>
                                        </div>
                                        <div class="col-md-9" style="padding: 0%;">
                                            <input type="text" class="form-control" name="husband_spousename"
                                                ng-model="$parent.husband_spousename"
                                                placeholder="Enter husband/spouse Fathers name"
                                                onkeypress='nameFilter(event)' required>
                                        </div>
                                    </div>
                                </div><br />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel panel-primary" id="allShow" class="tab-pane fade in active"
                    style="border-radius: 30px;">
                    <div class="panel-heading" style="border-radius: 30px;">
                        <span><strong>
                                <center>Present Address</center>
                            </strong>
                        </span>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="table col-lg-12 col-xs-12 ">
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="name"><b>State<span style="color: red">*</span>:</b></label>
                                        <input type="text" ng-disabled="!psstate" name="psstate" id="psstate"
                                            class="form-control" value="Nagaland">
                                    </div>
                                    <div class="col-md-4">
                                        <label for="name"><b>District<span style="color: red">*</span>:</b></label>
                                        <select class="form-control" ng-options="x.districtName for x in psdistricts"
                                            ng-model="psdistrict" name="psdistrict" ng-change="loadPsSubdivisions()"
                                            required>
                                            <option value="" disabled>--District--</option>
                                        </select>
                                        <span style="color:red"
                                            ng-show="myForm.psdistrict.$dirty && myForm.psdistrict.$invalid">
                                            <span ng-show="myForm.psdistrict.$error.required">Choose Your
                                                District.</span>
                                        </span>
                                    </div>

                                    <div class="col-md-4">
                                        <label for="Pin Code"><b>Sub Division<span
                                                    style="color: red">*</span>:</b></label>

                                        <select class="form-control" ng-options="x.subdivName for x in psSubdivList"
                                            ng-model="pssubdivision" name="pssubdivision" required>
                                            <option value="" disabled>--Sub Division--</option>
                                        </select>
                                    </div>


                                </div><br />
                                <div class="row">

                                    <div class="col-md-4">
                                        <label for="gender"><b>Police Station<span
                                                    style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="pspolicestation"
                                            placeholder="Enter Police station name" name="pspolicestation"
                                            onkeypress='nameNumber(event)' required>
                                        <span style="color:red"
                                            ng-show="myForm.pspolicestation.$dirty && myForm.pspolicestation.$invalid">
                                            <span ng-show="myForm.pspolicestation.$error.required">Police Station is
                                                required.</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Post Office<span
                                                    style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="pspostoffice"
                                            placeholder="Enter Post Office name" name="pspostoffice"
                                            onkeypress='nameNumber(event)' required>
                                        <span style="color:red"
                                            ng-show="myForm.pspostoffice.$dirty && myForm.pspostoffice.$invalid">
                                            <span ng-show="myForm.pspostoffice.$error.required">Post Office Name is
                                                required.</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for=""><b>Village<span style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="psvillege"
                                            placeholder="Enter Village name" name="psvillege"
                                            onkeypress='nameNumber(event)' required>
                                        <span style="color:red"
                                            ng-show="myForm.psvillege.$dirty && myForm.psvillege.$invalid">
                                            <span ng-show="myForm.psvillege.$error.required">Village is required.</span>
                                        </span>
                                    </div>
                                </div><br />
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Land Mark:</b></label>
                                        <input type="text" class="form-control" ng-model="pslandmark"
                                            placeholder="Enter Near Land Mark" name="pslandmark"
                                            onkeypress='nameNumber(event)'>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Colony<span style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="pscolony"
                                            placeholder="Enter Colony" name="pscolony" required>
                                        <span style="color:red"
                                            ng-show="myForm.pscolony.$dirty && myForm.pscolony.$invalid">
                                            <span ng-show="myForm.pscolony.$error.required">Enter your colony</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Block / Lane:</b></label>
                                        <input type="text" class="form-control" ng-model="psblock_lane"
                                            placeholder="Enter Block/Lane" name="psblock_lane">
                                    </div>
                                </div><br />
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Plot No / House No:</b></label>
                                        <input type="text" class="form-control" ng-model="psplotno"
                                            placeholder="Enter Plot No / House No" name="psplotno">
                                    </div>

                                    <div class="col-md-4">
                                        <label for="Phone"><b>Pincode<span style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="pspincode"
                                            placeholder="Enter Pincode" name="pspincode" maxlength="6"
                                            ng-pattern="/^[1-9][0-9]{5}$/" allow-numbers-only
                                            onkeypress='pincode(event)' required>
                                        <span ng-show="myForm.pspincode.$error.pattern" style="color:red">Invalid Pin
                                            Code.</span>
                                        <span style="color:red"
                                            ng-show="myForm.pspincode.$dirty && myForm.pspincode.$invalid">
                                            <span ng-show="myForm.pspincode.$error.required">Pincode is required.</span>
                                        </span>
                                    </div>
                                </div><br />
                            </div><br />
                        </div>
                        </table>
                    </div>
                </div>
                <div class="panel panel-primary" id="allShow" class="tab-pane fade in active"
                    style="border-radius: 30px;">
                    <div class="panel-heading" style="border-radius: 30px;">
                        <span><strong>
                                <center>Native Address</center>
                            </strong>
                        </span>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="table col-lg-12 col-xs-12 ">
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="name"><b>State<span style="color: red">*</span>:</b></label>
                                        <select class="form-control" ng-options="x.stateName for x in states"
                                            name="prstate" ng-model="prstate" ng-change="loadPrDistricts()" required>
                                            <option value="" disabled>--State--</option>
                                        </select>
                                        <span style="color:red"
                                            ng-show="myForm.prstate.$dirty && myForm.prstate.$invalid">
                                            <span ng-show="myForm.prstate.$error.required">Choose Your State.</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="name"><b>District<span style="color: red">*</span>:</b></label>
                                        <select class="form-control" ng-options="x.districtName for x in prdistricts"
                                            ng-model="prdistrict" name="prdistrict" ng-change="loadPrBlocks()" required>
                                            <option value="" disabled>--District--</option>
                                        </select>
                                        <span style="color:red"
                                            ng-show="myForm.prdistrict.$dirty && myForm.prdistrict.$invalid">
                                            <span ng-show="myForm.prdistrict.$error.required">Choose Your
                                                District.</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label><b>Sub Division<span style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="prsubdivision"
                                            placeholder="Enter Your Sub-Division" name="prsubdivision"
                                            onkeypress='nameNumber(event)' required>
                                    </div>

                                </div><br />
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="gender"><b>Police Station<span
                                                    style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="prpolicestation"
                                            placeholder="Enter Police Station name" name="prpolicestation"
                                            onkeypress='nameNumber(event)' required>
                                        <span style="color:red"
                                            ng-show="myForm.prpolicestation.$dirty && myForm.prpolicestation.$invalid">
                                            <span ng-show="myForm.prpolicestation.$error.required">Police Station is
                                                required.</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Post Office<span
                                                    style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="prpostoffice"
                                            placeholder="Enter Post Office name" name="prpostoffice"
                                            onkeypress='nameNumber(event)' required>
                                        <span style="color:red"
                                            ng-show="myForm.prpostoffice.$dirty && myForm.prpostoffice.$invalid">
                                            <span ng-show="myForm.prpostoffice.$error.required">Post Office Name is
                                                required.</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for=""><b>Village<span style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="prvillege"
                                            placeholder="Enter Village Name" name="prvillege"
                                            onkeypress='nameNumber(event)' required>
                                        <span style="color:red"
                                            ng-show="myForm.prvillege.$dirty && myForm.prvillege.$invalid">
                                            <span ng-show="myForm.prvillege.$error.required">Village is required.</span>
                                        </span>
                                    </div>
                                </div><br />
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Land Mark:</b></label>
                                        <input type="text" class="form-control" ng-model="prlandmark"
                                            placeholder="Enter Landmark" name="prlandmark"
                                            onkeypress='nameNumber(event)'>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Colony<span style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="prcolony"
                                            placeholder="Enter Colony" name="prcolony" required>
                                        <span style="color:red"
                                            ng-show="myForm.prcolony.$dirty && myForm.prcolony.$invalid">
                                            <span ng-show="myForm.prcolony.$error.required">Enter your Colony
                                                Name.</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Block / Lane:</b></label>
                                        <input type="text" class="form-control" ng-model="prblock_lane"
                                            placeholder="Enter Block/Lane" name="prblock_lane">
                                        </span>
                                    </div>
                                </div><br />
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="fathername"><b>Plot No / House No:</b></label>
                                        <input type="text" class="form-control" ng-model="prplotno"
                                            placeholder="Enter plot No/ House No" name="prplotno">
                                    </div>
                                    <div class="col-md-4">
                                        <label for="Phone"><b>Pincode<span style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" ng-model="prpincode"
                                            placeholder="Enter Pincode" name="prpincode" maxlength="6"
                                            onkeypress='pincode(event)' required ng-pattern="/^[1-9][0-9]{5}$/"
                                            allow-numbers-only>
                                        <span ng-show="myForm.prpincode.$error.pattern" style="color:red">Invalid Pin
                                            Code.</span>
                                        <span style="color:red"
                                            ng-show="myForm.prpincode.$dirty && myForm.prpincode.$invalid">
                                            <span ng-show="myForm.prpincode.$error.required">Pincode is required.</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="Phone"><b>Upload Address Proof:<span
                                                    style="color: red">*</span>:</b></label>
                                        <input type="file" accept=" image/jpg, image/jpeg, application/pdf"
                                            name="nativeAddressProof" id="nativeAddressProof" class="form-control"
                                            onchange="angular.element(this).scope().nativeAddressProof()">
                                        <div ng-show="showNativeAddressProofError">
                                            <span style="color: red;">{{nativeAddressProofFileGood}}</span>
                                        </div>
                                    </div>
                                </div><br />
                            </div><br />
                        </div>
                        </table>
                    </div>
                </div>
                <div class="panel panel-primary" id="allShow" class="tab-pane fade in active"
                    style="border-radius: 30px;">
                    <div class="panel-heading" style="border-radius: 30px;">
                        <span><strong>
                                <center>Others</center>
                            </strong>
                        </span>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="table col-lg-12 col-xs-12 ">
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="name"><b>Occupation<span style="color: red">*</span>:</b></label>
                                        <input type="text" class="form-control" name="occupication"
                                            ng-model="occupication" placeholder="Enter Occupication"
                                            onkeypress='nameFilter(event)' required>
                                        <span style="color:red"
                                            ng-show="myForm.occupication.$dirty && myForm.occupication.$invalid">
                                            <span ng-show="myForm.occupication.$error.required">Enter your
                                                occupication.</span>
                                        </span>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="name"><b>Religion<span style="color: red">*</span>:</b></label>
                                        <select class="form-control" name="religion" ng-model="religion" required>
                                            <option value="Christan">CHRISTAN</option>
                                            <option value="Hindu">HINDU</option>
                                            <option value="Muslim">MUSLIM</option>
                                            <option value="Sikh">SIKH</option>
                                            <option value="Jain">JAIN</option>
                                            <option value="Other">OTHER</option>
                                        </select>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="Phone"><b>Date Since Residing In District Of Dimapur<span
                                                    style="color: red">*</span>:</b></label>
                                        <input type="date" class="form-control" name="doresiding" ng-model="doresiding"
                                            required>
                                        <span style="color:red"
                                            ng-show="myForm.doresiding.$dirty && myForm.doresiding.$invalid">
