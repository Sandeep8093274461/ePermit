<script>
    var regNo = "<%= regNo %>";
    function loadPage(content, callback) {
        var mywindow = window.open('', 'Print', 'height=600,width=800');
        mywindow.document.write('<html><head><title>ILP CERTIFICATE FOR FULL YEAR </title><meta name="username" content="print">');
        mywindow.document.write('</head><body ng-app="myApp" ng-controller="myCtr">');
        mywindow.document.write(content);
        mywindow.document.write('</body></html>');
        callback(mywindow);
    }
    function printElem() {
        var content = document.getElementById('sgDetails').innerHTML;
        loadPage(content, function (mywindow) {
            mywindow.document.close();
            setTimeout(function () {
                mywindow.focus()
                mywindow.print();
                mywindow.close();
            }, 1000);
            return true;
        })
    }
</script>

