
<%- include('common/header.ejs') -%>

    <div class="content-wrapper">
        <div class="row">
          <div class="col-sm-4">
            
          </div>
          <div class="col-sm-4" style="margin-top: 10px;">
            
              <div class="panel-body" style="height: auto; margin:0px; background-color: #b3b5b8; ">
                <div class="row" >
                  <div class="col-lg-12 col-xs-12 ">
                    <label for="fathername"><b>Category <span
                      style="color: red">*</span>:</b></label><br>
                      <!-- <select class="form-control" ng-options="x.catagory for x in categori"
                      name="categor" ng-model="categor" ng-change="subCategorize()" >
                      <option value="" disabled>--category--</option>
                  </select> -->
                    <select class="form-control" style="  background-color: rgb(203, 238, 203);" ng-model="category"
                      ng-change="getSubCategory()" aria-placeholder="Select Category">
                      <option value="" disabled selected>Category</option>
                      <option ng-repeat="i in categori" value="{{i}}">{{i.catagory}}</option>
                    </select>
                </div><br> <br>
                  <div class="col-lg-12 col-xs-12 ">
                    <label for="fathername"><b>Sub Category <span
                      style="color: red">*</span>:</b></label><br>
                      <select class="form-control" style="background-color: rgb(203, 238, 203);" ng-model="subCategori"
                      ng-change="getUrl()" aria-placeholder="Select Sub-Category">
    
                      <option value="" disabled selected>Sub-Category</option>
                      <option ng-repeat="b in subCategory" value="{{b}}">{{b.name}}</option>
                    </select>
                  </div><br><br>
                  <div class="col-lg-12 col-xs-12 ">
                    <label for="fathername"><b>URL <span
                      style="color: red">*</span>:</b></label><br>
                      <select class="form-control" style="background-color: rgb(203, 238, 203);" ng-model="url"
                      >
    
                      <option value="" disabled selected>Select Your Url</option>
                      <option ng-repeat="b in urllink" value="{{b}}">{{b.url}}</option>
                    </select>
                  </div><br><br>
                  <div class="col-lg-12 col-xs-12 ">
                    <label for="fathername"><b>Is Accesable ? <span
                      style="color: red">*</span>:</b></label><br>
                      <select class="form-control" style="background-color: rgb(203, 238, 203);" ng-model="accesable"
                      >
    
                      <option value="" disabled selected>Select </option>
                      <option value="yes">Yes</option>
                      <option value="no">No</option>
                    </select>
                  </div><br><br>
                  <div class="col-lg-12 col-xs-12 ">
                    <label for="fathername"><b>Host At <span
                      style="color: red">*</span>:</b></label><br>
                      <select class="form-control" style="background-color: rgb(203, 238, 203);" ng-model="host"
                      aria-placeholder="Select Sub-Category">
    
                      <option value="" disabled selected>Select Your Host</option>
                      <option value="Nic">Nic</option>
                      <option value="Private">Private</option>
                    </select>
                  </div><br><br>
                  <div class="col-lg-12 col-xs-12 ">
                    <label for="fathername"><b>Maintaince By Nic <span
                      style="color: red">*</span>:</b></label><br>
                      <select class="form-control" style="background-color: rgb(203, 238, 203);" ng-model="maintainace"
                      >    
                      <option value="" disabled selected>Select </option>
                      <option value="Yes">Yes</option>
                      <option value="No">No</option>
                    </select>
                  </div><br><br>
                  <div class="col-lg-12 col-xs-12 ">
                    <label for="fathername"><b>Remark <span
                      style="color: red">*</span>:</b></label><br>
                      <textarea name="" id="" cols="30" rows="2" ng-model="remark"></textarea>
                  </div><br><br>
                  <div class="col-lg-12 col-xs-12 ">                   
                      <center><button type="button" class="btn btn-success" 
                        ng-click="submit();">Submit</button></center>
                  </div><br><br>                 
                </div>              
              </div>
          </div>
          <div class="col-sm-4">            
          </div>
        </div>
      </div>
      <script src="../controller/user/catagorypage.js"></script>
      <%- include('common/footer.ejs') -%>